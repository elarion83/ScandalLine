{
  "version": 2,
  "functions": {
    "api/og.ts": {
      "runtime": "edge"
    }
  },
  "routes": [
    {
      "src": "/api/og",
      "dest": "/api/og.ts"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/assets/$1"
    },
    {
      "src": "/timeline/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
} 